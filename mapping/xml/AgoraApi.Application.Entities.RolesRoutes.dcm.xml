<?xml version="1.0" encoding="utf-8"?>
<doctrine-mapping xmlns="http://doctrine-project.org/schemas/orm/doctrine-mapping" xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance" xsi:schemaLocation="http://doctrine-project.org/schemas/orm/doctrine-mapping http://doctrine-project.org/schemas/orm/doctrine-mapping.xsd">
  <entity name="AgoraApi\Application\Entities\RolesRoutes" table="roles_routes">
    <indexes>
      <index name="fk_role_routes_role_id_roles_id_idx" columns="roles_id"/>
    </indexes>
    <id name="id" type="integer" column="id">
      <generator strategy="IDENTITY"/>
    </id>
    <field name="route" type="string" column="route" length="45" nullable="false">
      <options>
        <option name="fixed"/>
      </options>
    </field>
    <field name="updatedAt" type="datetime" column="updated_at" nullable="false">
      <options>
        <option name="default">CURRENT_TIMESTAMP</option>
      </options>
    </field>
    <field name="createdAt" type="datetime" column="created_at" nullable="false">
      <options>
        <option name="default">CURRENT_TIMESTAMP</option>
      </options>
    </field>
    <many-to-one field="roles" target-entity="AgoraApi\Application\Entities\Roles" fetch="LAZY">
      <join-columns>
        <join-column name="roles_id" referenced-column-name="id"/>
      </join-columns>
    </many-to-one>
  </entity>
</doctrine-mapping>
